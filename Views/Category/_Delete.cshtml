@model ProjectCategoryVM

    <form asp-action="delete" asp-controller="category" method="post">
        <input type="hidden" asp-for="@Model.Id" />
        
        <div class="row">
            <div class="col-3">Name:</div>
            <div class="col-9">@Model.Name</div>
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-danger"><i class="fa fa-check"></i>  Yes </button>
            <button id="btnno" type="button" class="btn btn-primary"><i class="fa fa-remove"></i> No </button>
        </div>
    </form>



